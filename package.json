{
  "name": "matter-js-serialize",
  "version": "0.19.6",
  "description": "Serialize and deserialize matter-js worlds",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "source": "src/index.ts",
  "type": "module",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
  },
  "private": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "microbundle",
    "publish-lib": "npm run build && npm publish --access public",
    "publish-beta": "npm run build && npm publish --tag beta --access public",
    "publish-dryrun": "npm run build && npm publish --dry-run --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abigpotostew/matter-js-serialize.git"
  },
  "keywords": [
    "typescript",
    "library",
    "matter-js"
  ],
  "author": "Stewart Bracken <me@stewart.codes>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abigpotostew/matter-js-serialize/issues"
  },
  "homepage": "https://github.com/abigpotostew/matter-js-serialize#readme",
  "devDependencies": {
    "@types/matter-js": "^0.19.5",
    "matter-js": "^0.19.0",
    "microbundle": "^0.15.1",
    "typescript": "5.3.2"
  }
}
